{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

		  "Nanophysics slides": {
			"prefix": "nanophysics slide",
			"body": "\\\\subsubsection{Slide $1}"
		  },
		  "Nanophysics section with day": {
			"prefix": "nanophysics title",
			"body": "\\\\section{Lecture $1}\n\\\\marginpar{ \\\\displaydate{date}. \\\\\\\\ Compiled:  \\\\\\\\\\ \\\\today. \\\\\\\\\\ \\\\vspace{0.2cm}}"
		  },
		  "Text Code": {
			"prefix": "text code",
			"body": "\\\\texttt{${1:name}}"
		  },
		  "Text Code Bold": {
			"prefix": "text code bold",
			"body": "{\\\\bfseries\\\\texttt{${1:name}}}"
		  },
		  "Quotation user defined": {
			"prefix": "quotation",
			"body": "\\\\begin{chapquote}{${1:name_author}}\n    ${2:quotation}\n\\\\end{chapquote}"
		  },
		  "noindent": {
			"prefix": "noindent",
			"body": "\\\\noindent $1"
		  },
		  "Subfile init": {
			"prefix": "subfile",
			"body": "\\\\documentclass[${1:../main/main.tex}]{subfiles}\n\\\\begin{document}\n$2\n\\\\end{document}"
		  },
		  "Date": {
			"prefix": "date",
			"body": "\\\\marginpar{ \\\\textbf{Lecture $3.} \\\\\\\\\\  \\\\displaydate{date}. \\\\\\\\\\ Compiled:  \\\\today.}"
		  },
		  "Subfile Lecture": {
			"prefix": "subfile lecture",
			"body": "\\\\documentclass[../main/main.tex]{subfiles}\n\n\\\\newdate{date}{$1}{$2}{2019}\n\n\\\\begin{document}\n\n\\\\marginpar{ \\\\textbf{Lecture $3.} \\\\\\\\\\  \\\\displaydate{date}. \\\\\\\\\\ Compiled:  \\\\today.}\n$4\n\\\\end{document}"
		  },
		  "SVG figure": {
			"prefix": "svgfig",
			"body": "\\\\begin{figure}[ht]\n  \\\\centering\n  \\\\incfig{figures/$1}\n  \\\\caption{$2}\n  \\\\label{fig:$1}\n\\\\end{figure}"
		  },
		  "Regular citation": {
