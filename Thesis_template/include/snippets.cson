# Your snippets
#
# Atom snippets allow you to enter a simple prefix in the editor and hit tab to
# expand the prefix into a larger code block with templated values.
#
# You can create a new snippet in this file by typing "snip" and then hitting
# tab.
#
# An example CoffeeScript snippet to expand log to console.log:
#
# '.source.coffee':
#   'Console log':
#     'prefix': 'log'
#     'body': 'console.log $1'
#
# Each scope (e.g. '.source.coffee' above) can only be declared once.
#
# This file uses CoffeeScript Object Notation (CSON).
# If you are unfamiliar with CSON, you can read more about it in the
# Atom Flight Manual:
# http://flight-manual.atom.io/using-atom/sections/basic-customization/#_cson
# LATEX - GENERAL

# ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

'.text.tex.latex':

# TEXT and FORMATTING

  'Boldface':
    'prefix': 'bold'
    'body': '\\\\textbf{$1}'

  'Cursive':
    'prefix': 'em'
    'body': '\\\\emph{$1}'

  'Color text':
    'prefix': 'color text'
    'body': '\\\\hlfancy{${1:color}}{$2} $3'

  'Text color':
    'prefix': 'text color'
    'body': '\\\\textcolor{${1:color}}{$2} $3'

  'Margin text':
    'prefix': 'margin text'
    'body': '\\\\marginpar{$1} $2'

  'noindent':
    'prefix': 'noindent'
    'body': '\\\\noindent $1'

  'Text Code':
    'prefix': 'text code'
    'body': '\\\\texttt{${1:name}}'

  'Text Code Bold':
    'prefix': 'text code bold'
    'body': '{\\\\bfseries\\\\texttt{${1:name}}}'

  'Itemize':
    'prefix': 'itemize'
    'body': '''
    \\\\begin{itemize}
    \\\\item $1
    \\\\end{itemize}
    '''
    'description': ''

  'Enumerate':
    'prefix': 'enumerate'
    'body': '''
    \\\\begin{enumerate}
    \\\\item $1
    \\\\end{enumerate}
    '''
    'description': ''

# THEOREMS, DEFINITIONS AND SO ON

  'Theorem':
    'prefix': 'theorem'
    'body': '''\\\\begin{mytheorem}[Theorem name]
    $1
    \\\\end{mytheorem}
    '''

  'Proposition':
    'prefix': 'proposition'
    'body': '''\\\\begin{myprop}[Proposition name]
    $1
    \\\\end{myprop}
    '''

  'Lemma':
    'prefix': 'lemma'
    'body': '''\\\\begin{mylemma}[Lemma name]
    $1
    \\\\end{mylemma}
    '''

  'Corollary':
    'prefix': 'corollary'
    'body': '''\\\\begin{mycorollary}[Corollary name]
    $1
    \\\\end{mycorollary}
    '''

  'Definition':
    'prefix': 'definition'
    'body': '''\\\\begin{mydef}[Definition name]
    $1
    \\\\end{mydef}
    '''

  'Remark':
    'prefix': 'remark'
    'body': '''\\\\begin{myremark}[Remark name]
    $1
    \\\\end{myremark}
    '''

  'Proof':
    'prefix': 'proof'
    'body': '''\\\\begin{proof}
    $1
    \\\\end{proof}
    '''

# LABELS and REFERENCES

  'Label':
    'prefix': 'label'
    'body': '\\\\id{$1}'

  'Reference':
    'prefix': 'reference'
    'body': '\\\\idref{$1}'

  'Equation label':
    'prefix': 'eqlabel'
    'body': '\\\\id{eqn:$1}'

  'Equation reference':
    'prefix': 'eqreference'
    'body': 'Eq. \\\\eqidref{$1}'

  'Regular citation':
    'prefix': 'cite'
    'body': '\\\\cite{$2}'


# FIGURES

  'Margin figure':
    'prefix': 'margin figure'
    'body': '''
      \\\\marginpar{
      \\\\includegraphics[width=\\\\marginparwidth]{../path/$1}
      \\\\captionof{figure}{\\\\label{fig:} Description}
      }
      '''

  'Subfigure':
    'prefix': 'subfigure'
    'body': '''
    \\\\begin{subfigure}
    \\\\centering
        \\\\includegraphics[width=1\\\\textwidth]{\\\\mainpath/chapters/$1}
        \\\\caption[Short description]{\\\\label{fig:$2} Description.}
    \\\\end{subfigure}%
    '''
    'description': 'Needs the package subcaption'

  'TwoFigure Subcaption':
    'prefix': 'twofiguresub'
    'body': '''
    \\\\begin{figure}[h!]
    \\\\begin{minipage}[c]{0.5\\\\linewidth}
    \\\\subfloat[][Description]{ \\\\includegraphics[width=0.8\\\\textwidth]{\\\\mainpath/chapters/$1}  \\\\label{fig:$2} }
    \\\\end{minipage}
    \\\\begin{minipage}[]{0.5\\\\linewidth}
    \\\\centering
    \\\\subfloat[][Description]{\\\\includegraphics[width=0.8\\\\textwidth]{\\\\mainpath/chapters/$3}  \\\\label{fig:$4} }
    \\\\end{minipage}
    \\\\caption{\\\\label{fig:} Description}
    \\\\end{figure}
    '''
    'description': ''

  'ThreeFigure Subcaption':
    'prefix': 'threefiguresub'
    'body': '''
    \\\\begin{figure}[h!]
    \\\\begin{minipage}[c]{0.3\\\\linewidth}
    \\\\subfloat[][Description]{ \\\\includegraphics[width=0.8\\\\textwidth]{\\\\mainpath/chapters/$1}  \\\\label{fig:$2} }
    \\\\end{minipage}
    \\\\begin{minipage}[]{0.3\\\\linewidth}
    \\\\centering
    \\\\subfloat[][Description]{\\\\includegraphics[width=0.8\\\\textwidth]{\\\\mainpath/chapters/$3}  \\\\label{fig:$4} }
    \\\\end{minipage}
    \\\\begin{minipage}[]{0.3\\\\linewidth}
    \\\\centering
    \\\\subfloat[][Description]{\\\\includegraphics[width=0.8\\\\textwidth]\\\\mainpath/chapters/$5}  \\\\label{fig:$6} }
    \\\\end{minipage}
    \\\\caption{\\\\label{fig:} Description}
    \\\\end{figure}
    '''
    'description': ''

  'Figure':
    'prefix': 'figure'
    'body': '''
    \\\\begin{figure}[h!]
    \\\\centering
    \\\\includegraphics[width=1\\\\textwidth]{\\\\mainpath/chapters/$1}
    \\\\caption[Short description]{\\\\label{fig:} Description.}
    \\\\end{figure}
    '''
    'description': ''

  'TwoFigure':
    'prefix': 'twofigure'
    'body': '''
    \\\\begin{figure}[h!]
    \\\\begin{minipage}[c]{0.5\\\\linewidth}
    \\\\centering
    \\\\includegraphics[width=1\\\\textwidth]{\\\\mainpath/chapters/$1}
    \\\\end{minipage}
    \\\\begin{minipage}[]{0.5\\\\linewidth}
    \\\\centering
    \\\\includegraphics[width=1\\\\textwidth]{\\\\mainpath/chapters/$2}
    \\\\end{minipage}
    \\\\caption[Short description]{\\\\label{fig:$3} Description.}
    \\\\end{figure}
    '''
    'description': ''

  'Tikzpicture':
    'prefix': 'tikzpicture'
    'body': '''
    \\\\begin{figure}[h!]
    \\\\centering
    \\\\begin{tikzpicture}[scale=1, transform shape]
    $1
    \\\\end{tikzpicture}
    \\\\caption{\\\\label{fig:$3} Description.}
    \\\\end{figure}
    '''
    'description': ''

  'TwoFigure Tikz':
    'prefix': 'twofiguretikz'
    'body': '''
    \\\\begin{figure}[h!]
    \\\\begin{minipage}[c]{0.5\\\\linewidth}
    \\\\centering
    \\\\begin{tikzpicture}[scale=1, transform shape]
    $1
    \\\\end{tikzpicture}
    \\\\end{minipage}
    \\\\begin{minipage}[]{0.5\\\\linewidth}
    \\\\centering
    \\\\begin{tikzpicture}[scale=1, transform shape]
    $2
    \\\\end{tikzpicture}
    \\\\end{minipage}
    \\\\caption{\\\\label{fig:$3} Description.}
    \\\\end{figure}
    '''
    'description': ''

  'Figure and text':
    'prefix': 'figuretext'
    'body': '''
    \\\\begin{minipage}[c]{0.5\\\\linewidth}
    $1
    \\\\end{minipage}
    \\\\begin{minipage}[]{0.5\\\\linewidth}
    \\\\centering
    \\\\includegraphics[width=1\\\\textwidth]{\\\\mainpath/chapters/$2}
    \\\\captionof{figure}{\\\\label{} Description.}
    \\\\end{minipage}
    '''
    'description': ''


# EQUATIONS

  'Subequations numbered with letters':
    'prefix': 'subequations'
    'body': '''
      \\\\begin{subequations}
      \\\\begin{align}
        ${1:$2 &= $3 }
      \\\\end{align}
      \\\\end{subequations}
    '''
    'description': ''


  'Multiline equation':
    'prefix': 'multiline equation'
    'body': '''
      \\\\begin{equation}
      \\\\begin{split}
      $2  ${1:&=} $3 \\\\\\\\
      \\\\end{split}
      \\\\end{equation}
    '''

  'Equations numbered with the same number':
    'prefix': 'equations'
    'body': '''
    \\\\begin{equation}
        \\\\begin{gathered}
            $1
	   \\\\end{gathered}
    \\\\end{equation}
    '''
    'description': ''

  'Equations with cases':
    'prefix': 'cases equation'
    'body': '''
    \\\\begin{equation}
    $1
      \\\\begin{cases}
      $2 \\\\\\\\\\
      $3
      \\\\end{cases}
    \\\\end{equation}
    '''
    'description': ''

  'Equation':
    'prefix': 'equation'
    'body': '''
      \\\\begin{equation}
        $1
      \\\\end{equation}
    '''

  'SIunitx Value':
    'prefix': 'siunitx'
    'body': '\\\\SI{$1}{$2}'

  'SIunitx number':
    'prefix': 'number'
    'body': '\\\\num{$1}'

  'Inline math':
    'prefix': 'inline'
    'body': '\\\\( $1 \\\\)'

  'Schroedinger':
    'prefix': 'schroedinger'
    'body': 'Schr√∂dinger'


# LISTING

  'Simple Code':
    'prefix': 'code simple'
    'body': '''
    \\\\begin{lstlisting}[style=$1]
    $2
    \\\\end{lstlisting} '''


