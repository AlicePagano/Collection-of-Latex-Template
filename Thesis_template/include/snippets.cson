  'sign function':
    'prefix': 'sign'
    'body': '\\\\text{sign} $1'

  'Substack':
    'prefix': 'substack'
    'body': '\\\\substack{$1 \\\\\\\\ $2} $3'

  'Substack text':
    'prefix': 'substack text'
    'body': '\\\\substack{ \\\\text{$1} \\\\\\\\  \\\\text{$2} } $3'

  'Mathcal':
    'prefix': 'mathcal'
    'body': '\\\\mathcal{$1}'

  'Exp text':
    'prefix': 'exp text'
    'body': '\\\\exp ($1) $2'

  'Limit':
    'prefix': 'limit'
    'body': '\\\\lim_{$1} $2'

  'Curly braces':
    'prefix': 'curly braces'
    'body': '\\\\{ $1 \\\\}  '

  'Subset':
    'prefix': 'subset'
    'body': '\\\\subset $1'
